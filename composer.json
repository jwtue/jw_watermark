{
  "name": "jwtue/jw_watermark",
  "type": "typo3-cms-extension",
  "description": "Watermark Extension for TYPO3, adding a ViewHelper that creates watermarks.",
  "homepage": "https://github.com/jwtue/jw_watermark",
  "license": "MIT",
  "keywords": [
    "TYPO3 CMS",
    "watermark",
    "viewhelper"
  ],
  "support": {
    "issues": "https://github.com/jwtue/jw_watermark/issues"
  },
  "require": {
    "typo3/cms-core": "^12.4",
    "php": ">=8.0.0"
  },
  "autoload": {
    "psr-4": {
      "JwTue\\Watermark\\": "Classes"
    }
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "jw_watermark"
    }
  }
}
